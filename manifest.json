{
    "name": "Ratings for Netflix",
    "version": "1.0",
    "description": "Show IMDB Ratings for Netflix",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["https://www.netflix.com/*"],
            "js": ["js/content.js"],
            "css": ["css/content.css"],
            "run_at": "document_end"
        }
    ],
    "permissions": [
      "webRequest",
      "*://*.media-imdb.com/",
      "*://*.imdb.com/"
    ]
}